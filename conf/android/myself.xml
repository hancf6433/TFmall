<?xml version="1.0" encoding="utf-8" ?>

<!--天府手机银行"我的"页面-->
<page name="myself" xmlns="http://www.w3school.com.cn"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.w3school.com.cn ../xsd/ui.xsd">
    <!--已登录页面-->
    <ui name="我的页面设置按钮">poot(text="设置")</ui>
    <ui name="我的页面退出登录按钮">poot(resource_id="com.ionicframework.cgbank122507:id/btn_login")</ui>
    <ui name="我的页面确定退出按钮">poot(text="确定")</ui>


    <!--未登录-有手机号页面-->
    <ui name="登录页面手机号">poot(textM="1.*")</ui>
    <ui name="登录页面更多登录按钮">poot(text="更多登录")</ui>
    <ui name="登录页面切换账户按钮">poot(text="切换账户")</ui>

    <!--未登录-欢迎回来页面-->
    <ui name="登录页面欢迎回来">poot(text="手机号/熊猫金融账号")</ui>

    <!--账号，密码，协议-->
    <ui name="登录页用户名输入框">poot(resource_id="com.ionicframework.cgbank122507:id/tv_hint_account")</ui>
    <ui name="登录页密码输入框">poot(resource_id="com.ionicframework.cgbank122507:id/tv_hint_pwd")</ui>
    <ui name="登录页同意协议按钮">poot(resource_id="com.ionicframework.cgbank122507:id/cb_agree")</ui>
    <ui name="登录页登录按钮">poot(text="登录")</ui>

    <!--登录成功后提示信息不再提醒-->
    <ui name="登录成功不再提醒按钮">poot(text="不再提醒")</ui>





    <ui name="我的页面我的消息按钮">poot(text="我的消息")</ui>
    <ui name="我的页面我的收藏按钮">poot(text="我的收藏")</ui>
    <ui name="我的页面我上传的文件">poot(text="我上传的文件")</ui>
