    <ui name="我的姓名">poot(text="林莹")</ui>
    <ui name="头像">poot(text="头像")</ui>
    <ui name="右上角更多按钮">poot(resource_id="com.minxing.client:id/title_right_more")</ui>
    <ui name="拍照">poot(text="拍照")</ui>
