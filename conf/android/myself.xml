    <ui name="我的页面我下载的文件">poot(text="我下载的文件")</ui>
    <ui name="我的页面我的头像">poot(resource_id="com.minxing.client:id/slid_avatar")</ui>
    <ui name="我的个人信息页面">poot(text="个人信息")</ui>
